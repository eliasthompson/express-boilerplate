{
  "name": "express-boilerplate",
  "version": "0.1.0",
  "description": "An Express boilerplate to handle getting started on most applications.",
  "keywords": [
    "boilerplate",
    "esdoc",
    "eslint",
    "express",
    "mocha",
    "sequelize"
  ],
  "homepage": "https://github.com/eliasthompson/express-boilerplate",
  "bugs": "https://github.com/eliasthompson/express-boilerplate/issues",
  "license": "MIT",
  "author": {
    "name": "Elias Thompson",
    "email": "email@eliasthompson.com",
    "url": "https://github.com/eliasthompson"
  },
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliasthompson/express-boilerplate.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "nodemon --watch ./src",
    "lint": "eslint \"./*.js\" \"./**/*.js\"",
    "test": "npm run lint --silent && ./node_modules/mocha/bin/_mocha",
    "test:watch": "nodemon --ext .js -w ./src/ -w ./test/ --exec \"npm run test --silent || true\""
  },
  "devDependencies": {
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-node": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^5.2.0",
    "nock": "^9.0.2",
    "nodemon": "^1.11.0",
    "require-dir": "^0.3.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "common-errors": "^1.0.0",
    "config": "^1.30.0",
    "cors": "^2.7.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.13.1",
    "nodemailer": "^4.6.8",
    "pg": "^6.2.4",
    "sequelize": "^4.7.4",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "winston": "^3.1.0"
  }
}
